@model List<Starwars.Core.Entities.Jedi>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}

<h2>Jedis Search</h2>

<table>
    <thead>
        <tr>
            <th>Jedi name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var jedi in Model)
        {
            <tr>
                <td>@jedi.Name (Id: @jedi.JediId)</td>
            </tr>
        }
    </tbody>
   
</table>
