@{
    var demo = 4 + 5;
    ViewData["Title"] = $"Demo {demo}";
}

<h1>@ViewData["Title"]</h1>


<form method="post" action="">
<ul>
@for (var i = 0; i < demo; i++)
{
    <li>
         <input type="text" name="@($"txt{i}")" value="@i" /> 
    </li>
    
}
</ul>

<input type="submit" name="btnEnviar" value="Enviar" />
</form>